{
  "name": "localmart",
  "version": "1.0.0",
  "description": "Local marketplace platform for local shops",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client-new && npm start",
    "client:build": "cd client-new && npm run build",
    "build": "npm run client:build",
    "start": "node server/index.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express-validator": "^7.0.1",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0"
  },
  "keywords": ["marketplace", "local", "shops", "ecommerce"],
  "author": "",
  "license": "MIT"
}
